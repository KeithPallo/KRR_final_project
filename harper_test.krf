;;; -----Horn Clauses-----

;;; findBalance: finds course more similar to liked course than a disliked course
(<== (findBalance ?caseGood ?caseBad ?caseLib ?caseRec) 
     (reminding ?caseBad ?caseLib (TheSet) ?case1 ?match1) 
     (reminding ?caseGood 
       (CaseLibrarySansFn ?caseLib ?case1) (TheSet) ?caseRec 
       ?match2))

;;; betterProfVsInterest: finds courses based on prof and topic similarity and 
;;; returns one most similar to a liked course.  Sadly, this does not work. :(
(<== (betterProfVsInterest ?courseGood ?caseLib ?courseRec) 
     (reminding ?courseGood ?caseLib 
       (requiredCorrespondence 
         (courseInstructor ?course1 WillieWilson) 
         (courseInstructor ?course2 WillieWilson)) ?case1 
       ?match1) 
     (reminding ?courseGood ?caseLib 
       (requiredCorrespondence 
         (breadthInDepth ?course1 
           ArtificialIntelligence-Topic) 
         (breadthInDepth ?course3 
           ArtificialIntelligence-Topic)) ?case2 ?match2) 
     (different ?case1 ?case2) 
     (isa TempTestLib000ForQuery CaseLibrary) 
     (CaseLibraryContains TempTestLib000ForQuery ?case1) 
     (CaseLibraryContains TempTestLib000ForQuery ?case2) 
     (reminding ?courseGood TempTestLib000ForQuery (TheSet) 
       ?courseRec ?match)) context = CSAcademicAdvisorMt; action = Store